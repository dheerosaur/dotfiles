# PS1='$(__git_ps1 "[%s] ")\W \$ '
PS1='$(__git_ps1 "[%s] ")\W $ '

if [ -f /etc/bash_completion ]; then
    source /etc/bash_completion
fi

if [ -f /etc/bash_completion.d/git ]; then
    source /etc/bash_completion.d/git
fi

[ -s "$HOME/bin/complete/tmuxinator.bash" ] && source ~/bin/complete/tmuxinator.bash

# The next line updates PATH for the Google Cloud SDK.
# if [ -f '/Users/dheerosaur/Downloads/software/google-cloud-sdk/path.bash.inc' ]; then source '/Users/dheerosaur/Downloads/software/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
# if [ -f '/Users/dheerosaur/Downloads/software/google-cloud-sdk/completion.bash.inc' ]; then source '/Users/dheerosaur/Downloads/software/google-cloud-sdk/completion.bash.inc'; fi

if [ -f $(brew --prefix)/etc/bash_completion ]; then
    source $(brew --prefix)/etc/bash_completion
fi

